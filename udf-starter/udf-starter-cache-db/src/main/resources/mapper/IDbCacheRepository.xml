<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.itkk.udf.starter.cache.db.repository.IDbCacheRepository">
    <delete id="clearExpireCahce">
        DELETE
        FROM TF_DB_CACHE A
        WHERE A.EXPIRE_TIME <![CDATA[ < ]]> REPLACE(unix_timestamp(NOW(3)),'.','')
    </delete>
</mapper>
